{
  "manifest_version": 3,
  "name": "RocketMoney Transaction Extractor",
  "version": "1.0",
  "description": "Extract transactions from RocketMoney.com and add them to Workday expense reports",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://app.rocketmoney.com/*",
    "https://*.myworkday.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://app.rocketmoney.com/*"],
      "js": ["content.js"]
    },
    {
      "matches": ["https://*.myworkday.com/*"],
      "js": ["workday.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 